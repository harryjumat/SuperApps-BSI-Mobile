		wait 2
		Device("Device").App("Bayar_Beli").MobileEdit("Input masukkan nominal").Set "144000"
		wait 2
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Nominal Bayar", "Masukkan nominal bayar", ReportStatus.Passed)
		wait 2
		Device("Device").App("Bayar_Beli").MobileLabel("Button Lanjutkan & Konfirmasi").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Konfirmasi Transaksi", "Pastikan nama agen asuransi dan nominal pembayaran telah sesuai", ReportStatus.Passed)
		wait 2
		Device("Device").App("Bayar_Beli").MobileLabel("Button Lanjutkan & Konfirmasi").Tap
		wait 2
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Konfirmasi PIN", "Masukkan konfirmasi PIN", ReportStatus.Passed)
