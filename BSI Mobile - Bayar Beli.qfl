		wait 1
		Device("Device").App("Bayar_Beli").MobileObject("btnPin3").Tap
		wait 1
		Device("Device").App("Bayar_Beli").MobileObject("btnPin4").Tap
		wait 1
		Device("Device").App("Bayar_Beli").MobileObject("btnPin5").Tap
		wait 1
		Device("Device").App("Bayar_Beli").MobileObject("btnPin6").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Loading Konfirmasi PIN", "Loading konfirmasi PIN", ReportStatus.Passed)
		wait 5
		Device("Device").App("Bayar_Beli").MobileObject("Icon Arrow Detail Transaksi").Tap
		wait 2
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Transaksi Berhasil", "Tampilan transaksi berhasil", ReportStatus.Passed)
		wait 1
		
	ElseIf x = "Negative" Then
		
		'=======================SCRIPT NEGATIVE======================='
		
	End If
End Function

