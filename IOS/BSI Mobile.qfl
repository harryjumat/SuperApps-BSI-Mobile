Dim nomorFT
Function Login()
	Device("Device").App("Login").Launch
	AIUtil.SetContext Device("Device")
	Call DurationStart("Splashscreen")
	do 
	wait 0.1
	Loop until Device("Device").InsightObject("InsightObject Sholat").Exist
	Call DurationEnd("Prelogin","Splashscreen")
	
	Call CaptureImageUFTV2(Device("Device").App("Login"), "Login", "Berhasil masuk ke menu login, Klik Login", ReportStatus.Passed)
	
	Call DurationStart("Button Login")
	Device("Device").App("Login").MobileLabel("Login").Tap
	do
	wait 0.1
	Loop until Device("Device").App("Login").MobileEdit("Password").Exist
	Call DurationEnd("Prelogin","Button Login")
	
	Call CaptureImageUFTV2(Device("Device").App("Login"), "Input Password", "Berhasil masuk untuk memasukkan password, Masukkan Password", ReportStatus.Passed)
	
	Call DurationStart("Input Password")
	Device("Device").App("Login").MobileEdit("Password").Set "p@ssw0rd"
	Call DurationEnd("Prelogin","Input Password")
	
	wait 1
	Call CaptureImageUFTV2(Device("Device").App("Login"), "Password Valid", "Mengisikan password dengan benar, Klik Login", ReportStatus.Passed)
	
	Call DurationStart("Button Login")
	Device("Device").App("Login").MobileButton("Login").Tap
	Do
	wait 0.1
	Loop until Device("Device").App("Login").MobileLabel("Harry").Exist
	Call DurationEnd("Prelogin","Button Login")
	wait 5

End Function

Function Navigate(x)
	If x = "Bayar_Beli" Then
		Call Login()
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Dashboard", "Berhasil masuk ke halaman dashboard, Klik Bayar Beli", ReportStatus.Passed)
		wait 1
'		Device("Device").InsightObject("InsightObject btn bayar beli").Click
		AIUtil("button", "", micFromLeft, 2).Click
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Bayar & Beli", "Berhasil masuk ke halaman bayar/beli, Klik Tampilkan Lebih Banyak", ReportStatus.Passed)
		wait 3
		Device("Device").App("Bayar_Beli").MobileLabel("tampilkanlebihbanyak").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Bayar & Beli", "Berhasil menampilkan lebih banyak menu", ReportStatus.Passed)
		wait 1
	ElseIf x = "Berbagi" Then
		Call Login()
		Call CaptureImageUFTV2(Device("Device").App("Berbagi"), "Dashboard", "Berhasil masuk ke halaman dashboard, Klik Ziswaf", ReportStatus.Passed)
		wait 1
		If Device("Device").InsightObject("InsightObject Dashboard - Ziswaf").Exist Then
			Device("Device").InsightObject("InsightObject Dashboard - Ziswaf").Click
			else
			AIUtil("button", "", micFromTop, 2).Click
		End If
		wait 3
		Call CaptureImageUFTV2(Device("Device").App("Berbagi"), "Ziswaf", "Berhasil masuk ke halaman Ziswaf", ReportStatus.Passed)
		wait 1
	ElseIf x = "Cardless Withdrawal" Then
		Call Login()
		Call CaptureImageUFTV2(Device("Device").App("Cardless Withdrawal"), "Dashboard", "Berhasil masuk ke halaman dashboard, Klik Ziswaf", ReportStatus.Passed)
		wait 1
		Device("Device").App("Cardless Withdrawal").MobileLabel("btn_megamenu").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Cardless Withdrawal"), "Mega Menu", "Berhasil menampilkan Mega Menu", ReportStatus.Passed)
		wait 1
	End If
End Function

Function LoginLimit()
	Device("Device").App("Login").Launch
	AIUtil.SetContext Device("Device")
	do 
	wait 1
	Loop until Device("Device").InsightObject("InsightObject Sholat").Exist
		Device("Device").App("Login").MobileLabel("Login").Tap
		wait 1
		Device("Device").App("Login").MobileEdit("Password").Set "p@ssw0rd"
		wait 1
'		Device("Device").App("Login").MobileButton("Masuk").Tap
		Device("Device").App("Login").MobileButton("Login").Tap
		Do
			wait 1
		Loop until Device("Device").App("Login").MobileLabel("Harry").Exist
End Function

