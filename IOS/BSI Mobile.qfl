Function PerizinanKamera(param)
	wait 1
	Device("Device").InsightObject("InsightObject buletan iphone").Click
	wait 1
	Device("Device").InsightObject("InsightObject iphone utama").Click
	wait 1
	If Device("Device").App("Home").MobileButton("Pengaturan").Exist Then
		Device("Device").App("Home").MobileButton("Pengaturan").Tap
	End If
	wait 1
	Device("Device").App("Settings").MobileList("MobileList Pengaturan").Swipe "up",205,430
	wait 2
	Device("Device").App("Settings").MobileList("MobileList Pengaturan").Swipe "up",205,430
	wait 2
	Device("Device").App("Settings").MobileList("MobileList Pengaturan").Swipe "up",205,430
	wait 2
	Device("Device").App("Settings").MobileList("MobileList Pengaturan").Swipe "up",205,430
	wait 2
	Device("Device").App("Settings").MobileList("MobileList Pengaturan").Swipe "up",205,430
	wait 2
	If Device("Device").App("Settings").MobileLabel("BSIMobile").Exist Then
		Device("Device").App("Settings").MobileLabel("BSIMobile").Tap
	End If
	wait 1
	Select Case param
	case "on"	Device("Device").InsightObject("InsightObject Kamera").Click
	case "off"	Device("Device").InsightObject("InsightObject Kamera Off").Click
	End Select
	wait 1
End Function

