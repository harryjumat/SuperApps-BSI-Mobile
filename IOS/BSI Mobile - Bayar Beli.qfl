Function TransaksiBPJSBPU()
	Call Navigate("Bayar_Beli")
	Device("Device").InsightObject("InsightObject btn bpjs").Click
	do
	wait 1
	Loop until Device("Device").InsightObject("InsightObject logo bpjs").Exist
	Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "BPJS", "Berhasil menampilkan halaman list BPJS", ReportStatus.Passed)
	wait 1
	Device("Device").App("Bayar_Beli").MobileLabel("Ketenagakerjaan BPU").Tap
	Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "BPJS", "Berhasil masuk ke halaman BPJS", ReportStatus.Passed)
	Device("Device").App("Dev").MobileEdit("nomor hp").Set nopel
	wait 1
	Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "BPJS", "Berhasil input nomor virtual account", ReportStatus.Passed)
	Device("Device").App("Dev").MobileButton("Selesai").Tap
	wait 1
	Device("Device").App("Supplier").MobileLabel("Jumlah bulan").Tap
	wait 1
	Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Jumlah Bulan", "Berhasil menampilkan pilihan jumlah bulan", ReportStatus.Passed)
	wait 1
	Device("Device").App("Bayar_Beli").MobileDropDown("MobileDropDown_2").Select "12",0	
	wait 1
	Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Jumlah Bulan", "Pilih jumlah bulan", ReportStatus.Passed)
	wait 1
	Device("Device").App("Dev").MobileLabel("Pilih").Tap
	wait 1
	Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Berhasil Pilih Jumlah Bulan", "Berhasil memilih jumlah bulan", ReportStatus.Passed)
	Device("Device").App("Bayar_Beli").MobileLabel("Selanjutnya").Tap
End Function

