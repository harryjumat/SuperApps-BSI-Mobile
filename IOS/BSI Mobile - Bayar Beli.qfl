Function BPJSTransaksi(nopel,counter)
		Call Navigate("Bayar_Beli")
		Device("Device").InsightObject("InsightObject btn bpjs").Click
		do
		wait 1
		Loop until Device("Device").InsightObject("InsightObject logo bpjs").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "BPJS", "Berhasil masuk ke halaman BPJS", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileLabel("Kesehatan").Tap
		do
		wait 1
		Loop until Device("Device").App("Dev").MobileLabel("Kesehatan").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "BPJS Kesehatan", "Berhasil masuk ke halaman BPJS Kesehatan", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileEdit("nomorva").Set nopel
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Nomor VA", "Berhasil memasukkan nomor va", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileLabel("btn_selanjutnya").Tap
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pembayaran BPJS Kesehatan", "Berhasil masuk ke halaman pembayaran BPJS Kesehatan", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileButton("datepicker_jumlahbulan").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Jumlah Bulan", "Berhasil menampilkan pilihan jumlah bulan", ReportStatus.Passed)
		wait 1
		For i = 1 To counter Step 1
			Device("Device").App("Dev").MobileDropDown("MobileDropDown Jumlah Bulan").Select "12",0
		Next
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Jumlah Bulan", "Pilih jumlah bulan", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileButton("btn_pilih").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Berhasil Pilih Jumlah Bulan", "Berhasil memilih jumlah bulan", ReportStatus.Passed) 
		wait 1
		Device("Device").App("Bayar_Beli").MobileButton("btn_lanjutkan").Tap
		wait 1
End Function
Function BPJSKesehatan(byval x)
	If x = "positive" Then
		Call BPJSTransaksi(nopel,2)
		Call Keluar(Device("Device").App("Dev").MobileLabel("Konfirmasi Transaksi"),"Bayar_Beli")
		do
		wait 1
		Loop until Device("Device").App("Dev").MobileLabel("Konfirmasi Transaksi").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Konfirmasi Transaksi", "Berhasil masuk ke halaman konfirmasi transaksi", ReportStatus.Passed)
		wait 1
		Device("Device").App("Dev").MobileLabel("Konfirmasi").Tap
		wait 1
		Call KonfirmasiPIN("Bayar_Beli")
		Call TransaksiNonSwipe("Bayar_Beli", nomorFT, "resi bpjs")
		Call cekMutasi("BPJS Kesehatan" , nomorFT)
		Call cekJurnal("BPJS Kesehatan", nomorFT)	
	ElseIf x = "negative" Then
		'LIMIT HARIAN
		Call SetLimitHarian("pembayaran")
		Call BPJSTransaksi(nopel,3)		
		wait 3
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Limit Harian", "Berhasil menampilkan notifikasi limit harian", ReportStatus.Passed)'Limit harian
		wait 1
		Call UnSetLimitHarian("pembayaran")
		'LIMIT TRANSAKSI
		Call SetLimitTransaksi("pembayaran")
		Call BPJSTransaksi(nopel,4)
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Limit Transaksi", "Berhasil menampilkan notifikasi limit transaksi", ReportStatus.Passed)'Limit transaksi
		wait 3
		Call UnSetLimitTransaksi("pembayaran")
		'=========================================================================================================
		Call BPJSTransaksi("123456789",8)
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Salah Nomor Pelanggan", "Berhasil menampilkan error 'nomor pelanggan yang dimasukan salah'", ReportStatus.Passed)
		wait 1
		'TAGIHAN SUDAH LUNAS
		Call BPJSTransaksi(nopel,2)
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Tagihan Sudah Lunas", "Berhasil menampilkan error 'tagihan sudah lunas''", ReportStatus.Passed) 
		wait 1
		Device("Device").App("Dev").MobileLabel("Kembali").Tap
		'SALDO TIDAK CUKUP
		wait 1
		Device("Device").InsightObject("InsightObject back").Click
		wait 1
		Device("Device").InsightObject("InsightObject silang").Click
		wait 1
		Device("Device").App("Bayar_Beli").MobileEdit("nomorva").Set nopel
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Nomor VA", "Berhasil memasukkan nomor va", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileLabel("btn_selanjutnya").Tap
		wait 10
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pembayaran BPJS Kesehatan", "Berhasil masuk ke halaman pembayaran BPJS Kesehatan'", ReportStatus.Passed) 
		wait 1
		Device("Device").App("Bayar_Beli").MobileButton("datepicker_jumlahbulan").Tap
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Jumlah Bulan", "Berhasil menampilkan pilihan jumlah bulan", ReportStatus.Passed)
		wait 1
		For i = 1 To 10 Step 1
			Device("Device").App("Dev").MobileDropDown("MobileDropDown Jumlah Bulan").Select "12",0
		Next
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Jumlah Bulan", "Pilih jumlah bulan", ReportStatus.Passed) 
		wait 1
		Device("Device").App("Bayar_Beli").MobileButton("btn_pilih").Tap
		wait 1
		Device("Device").App("Bayar_Beli").MobileLabel("Ubah").Tap
		wait 3
		Device("Device").InsightObject("InsightObject cari nama").Click
		wait 1
		Device("Device").Enterkeys typeValue, "909"
		wait 1
		Device("Device").App("Bayar_Beli").MobileLabel("Rp0,00").Tap
		wait 1
		Device("Device").App("Bayar_Beli").MobileButton("btn_lanjutkan").Tap
		Do 
		wait 1
		Loop until Device("Device").App("Dev").MobileLabel("Kembali").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Saldo Tidak Cukup", "Berhasil menampilkan error saldo tidak cukup", ReportStatus.Passed) 			
	End If
End Function
