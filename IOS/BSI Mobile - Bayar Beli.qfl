Function Institusi(x,payment)
	If x = "positive" Then
		If payment = "open"  Then
			TransaksiInstitusiOP(param)
			Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Konfirmasi Transaksi", "Berhasil menampilkan halaman konfirmasi transaksi", ReportStatus.Passed)
			Device("Device").App("Dev").MobileLabel("Konfirmasi").Tap
			wait 1
		ElseIf payment = "close"  Then
			TransaksiInstitusiCP(param)
		End If
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Konfirmasi Transaksi", "Berhasil masuk ke halaman konfirmasi transaksi", ReportStatus.Passed) 
		Device("Device").App("Dev").MobileLabel("Konfirmasi").Tap
		wait 1
		Call KonfirmasiPIN("Bayar_Beli")
		Call TransaksiNonSwipe("Bayar_Beli", nomorFT, "resi institusi")	
	ElseIf x = "negative" Then
		If payment = "open" Then
			InstitusiOPNegative(param)
		ElseIf payment = "close" Then
			InstitusiCPNegative(param)
		End If
	End If
End Function
