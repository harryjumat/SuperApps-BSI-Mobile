		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Bayar & Beli", "Berhasil masuk ke halaman bayar/beli", ReportStatus.Passed)
		wait 1
		Device("Device").App("Bayar_Beli").MobileLabel("tampilkanlebihbanyak").Tap
		wait 1
		Device("Device").InsightObject("InsightObject Bayar Beli - Aqiqah").Click
'		Do 
		wait 3
'		Loop until Device("Device").App("Dev").MobileLabel("Dompet Dhuafa Republika").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Supplier", "Berhasil masuk ke pilih supplier", ReportStatus.Passed)
		
		'supplier
		Select Case supplier
			Case "DompetDhuafa" DompetDhuafa.Tap
			Case "LaznasBSMUmat" LaznasBSMUmat.Tap
			Case "RumahZakatIndonesia" RumahZakatIndonesia.Tap
			Case "LAZISMU" LAZISMU.Tap
			Case "LaznasDewanDakwah" LaznasDewanDakwah.Tap
			Case "LaznasPPPA" LaznasPPPA.Tap
			Case "YatimMandiri" YatimMandiri.Tap
			Case "SinergiFoundation" SinergiFoundation.Tap
			Case "HumanInitiative" HumanInitiative.Tap
			Case "BAZNAS" BAZNAS.Tap
		End Select		
		
		'logo		
		Select Case logo
			Case "Logo_DompetDhuafa" 
				Do 
				wait 1
				Loop until Logo_DompetDhuafa.Exist
			Case "Logo_LaznasBSMUmat" 
				Do 
				wait 1
				Loop until Logo_LaznasBSMUmat.Exist
			Case "Logo_RumahZakatIndonesia" 
				Do 
				wait 1
				Loop until Logo_RumahZakatIndonesia.Exist
			Case "Logo_Dana" 
'				Do 
				wait 1
'				Loop until Logo_Dana.Exist
		End Select
		
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Hewan", "Berhasil masuk ke pilih hewan", ReportStatus.Passed)
		
		
