Function PulsaPaketDataTelkomsel(byval x)
	If x = "pulsapositive" Then
		Call Pulsa("telkomsel")
	ElseIf x = "pulsanegative" Then
		Call PulsaNegative("telkomsel")
	ElseIf x = "paketdatapositive" Then	
		Call PaketData("telkomsel")
	ElseIf x = "paketdatanegative" Then
		Call PaketDataNegative("telkomsel")
	End If	
End Function

Function PulsaPaketDataXL(byval x)
	If x = "pulsapositive" Then
		Call Pulsa("xl")
	ElseIf x = "pulsanegative" Then
		Call PulsaNegative("xl")
	elseIf x = "paketdatapositive" Then
		Call PaketData("xl")
	elseIf x = "paketdatanegative" Then
		Call PaketDataNegative("xl")
	End If
End Function 

 Function PulsaPaketDataIndosat(x)
 	If x = "pulsapositive" Then
