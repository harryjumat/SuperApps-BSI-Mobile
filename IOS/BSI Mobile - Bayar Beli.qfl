		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Berhasil Pilih Hewan", "Berhasil memilih hewan aqiqah", ReportStatus.Passed)
		Device("Device").App("Dev").MobileLabel("Pilih").Tap
		Do 
		wait 1
		Loop until Device("Device").App("Dev").MobileLabel("7420605239").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Form Aqiqah", "Berhasil masuk ke halaman pengisian form aqiqah", ReportStatus.Passed)
		
