 		Call PulsaNegative("indosat")
	ElseIf x = "paketdatapositive" Then
		Call PaketData("indosat")
	ElseIf x = "paketdatanegative" Then
		Call PaketDataNegative("indosat")
	End If
End Function

Function PulsaPaketDataTri(x)
	If x = "paketdatapositive" Then
		Call PaketData("tri")
	ElseIf x = "paketdatanegative" Then
		Call PaketDataNegative("tri")
	End If
End Function

Function PulsaTri(x)
	if x = "positive" then
		Call Pulsa("tri")
	ElseIf x = "negative" Then
		Call PulsaNegative("tri")
	End If
End Function

