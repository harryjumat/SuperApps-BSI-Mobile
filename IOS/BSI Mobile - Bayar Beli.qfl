Function Aqiqah(x,supplier, logo, hewan, gender, nama, toogle)
	If x = "positive" Then
		Call Navigate("Bayar_Beli")
		Device("Device").InsightObject("InsightObject Bayar Beli - Aqiqah").Click
		wait 3
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Supplier", "Berhasil masuk ke pilih supplier", ReportStatus.Passed)
		'supplier
		Select Case supplier
			Case "DompetDhuafa" 		Device("Device").App("Supplier").MobileLabel("Dompet Dhuafa Republika").Tap
			Case "LaznasBSMUmat" 		Device("Device").App("Supplier").MobileLabel("Laznas BSM Umat").Tap
			Case "RumahZakatIndonesia" 	Device("Device").App("Supplier").MobileLabel("Rumah Zakat Indonesia").Tap
			Case "LAZISMU" 				Device("Device").App("Supplier").MobileLabel("LAZISMU").Tap
			Case "LaznasDewanDakwah" 	Device("Device").App("Supplier").MobileLabel("Laznas Dewan Dakwah").Tap
			Case "LaznasPPPA"			Device("Device").App("Supplier").MobileLabel("Laznas PPPA Daarul Qur`a").Tap
			Case "YatimMandiri" 			Device("Device").App("Supplier").MobileLabel("Yatim Mandiri").Tap
			Case "SinergiFoundation" 		Device("Device").App("Supplier").MobileLabel("Sinergi Foundation").Tap
			Case "HumanInitiative" 		Device("Device").App("Supplier").MobileLabel("Human Initiative").Tap
			Case "BAZNAS" 				Device("Device").App("Supplier").MobileLabel("BAZNAS").Tap
		End Select		
		'logo		
		Select Case logo
			Case "Logo_DompetDhuafa" 
				Do 
				wait 1
				Loop until Device("Device").InsightObject("InsightObject Logo Dompet Dhuafa").Exist
			Case "Logo_LaznasBSMUmat" 
				Do 
				wait 1
				Loop until Device("Device").InsightObject("InsightObject Logo Laznas BSM Umat").Exist
			Case "Logo_RumahZakatIndonesia" 
				Do 
				wait 1
				Loop until Device("Device").InsightObject("InsightObject Logo Rumah Zakat Indonesia").Exist
			Case "Logo_Dana" 
'				Do 
				wait 1
'				Loop until Device("Device").InsightObject("InsightObject Logo Dana").Exist
		End Select
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Pilih Hewan", "Berhasil masuk ke pilih hewan", ReportStatus.Passed)		
		'HEWAN AQIQAH 
		Select Case hewan
			Case "Kambing" 	Device("Device").App("Supplier").MobileLabel("Kambing_2").Tap
		End Select	
		wait 1
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Berhasil Pilih Hewan", "Berhasil memilih hewan aqiqah", ReportStatus.Passed)
		Device("Device").App("Dev").MobileLabel("Pilih").Tap
		Do 
		wait 1
		Loop until Device("Device").App("Dev").MobileLabel("7420605239").Exist
		Call CaptureImageUFTV2(Device("Device").App("Bayar_Beli"), "Form Aqiqah", "Berhasil masuk ke halaman pengisian form aqiqah", ReportStatus.Passed)
		'GENDER
		Select Case gender
			Case "Pria" 		Device("Device").App("Dev").MobileLabel("Pria").Tap
			Case "Wanita" 	Device("Device").App("Dev").MobileLabel("Wanita").Tap
		End Select
		wait 1
		Device("Device").App("Dev").MobileLabel("Atas nama beserta Bin/Bi").Tap
		wait 1
		'NAMA
		Select Case nama
			Case "Fulan" 		Device("Device").Enterkeys typeValue, "Fulan bin Fulan"
			Case "Fulanah" 	Device("Device").Enterkeys typeValue, "Fulanah binti Fulan"
		End Select
		wait 1
		Device("Device").App("Supplier").MobileButton("Selesai_3").Tap
		wait 1
		'TOOGLE
		Select Case toogle
			Case "Aktif" 		Device("Device").App("Dev").MobileToggle("MobileToggle").Tap
			Case "NonAktif" 	Device("Device").App("Supplier").MobileLabel("Detail Penerima Laporan").Tap
		End Select
		wait 2
