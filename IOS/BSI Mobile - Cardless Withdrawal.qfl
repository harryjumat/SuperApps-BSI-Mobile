	Loop until Device("Device").App("Cardless Withdrawal").MobileLabel("Buat Token Baru").Exist
	Call CaptureImageUFTV2(Device("Device").App("Cardless Withdrawal"), "Buat Token Baru", "Berhasil menampilkan Lokasi Penarikan, pilih nominal yang akan ditarik", ReportStatus.Passed)
